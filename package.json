{
  "private": true,
  "name": "mpreorders",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run -s serve || npm run -s dev",
    "build": "preact build",
    "serve": "preact build && preact serve",
    "dev": "preact watch",
    "lint": "eslint src"
  },
  "eslintConfig": {
    "extends": "eslint-config-synacor",
    "rules": {
      "no-unused-vars": "warn",
      "react/sort-comp": "off",
      "lines-around-comment": "off",
      "react/prefer-stateless-function": "off"
    }
  },
  "eslintIgnore": [
    "build/*"
  ],
  "devDependencies": {
    "eslint": "^4.16.0",
    "eslint-config-synacor": "^1.1.0",
    "if-env": "^1.0.4",
    "preact-cli": "^2.0.0",
    "preact-cli-sw-precache": "^1.0.3",
    "webpack-dotenv-plugin": "^2.0.2"
  },
  "dependencies": {
    "abortcontroller-polyfill": "^1.0.9",
    "double-ended-queue": "^2.1.0-0",
    "fetch-polyfill": "^0.8.2",
    "preact": "^8.2.1",
    "preact-async-route": "^2.1.0",
    "preact-compat": "^3.18.0",
    "preact-custom-scrollbars": "^4.0.4",
    "preact-feather": "^1.0.2",
    "preact-material-components": "^1.3.5",
    "preact-router": "^2.5.5",
    "preact-virtual-list": "^0.3.1",
    "pubsub-js": "^1.6.0",
    "react-icons": "^2.2.7",
    "sw-precache-webpack-dev-plugin": "^0.2.0",
    "universal-localstorage": "",
    "uuid": "^3.2.1",
    "webpack-dev-server": "^2.11.1",
    "whatwg-fetch": "^2.0.3",
    "window-or-global": "^1.0.1"
  }
}
